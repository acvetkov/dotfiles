#!/usr/bin/env bash

# Exports
export HOMEBREW_CASK_OPTS="--appdir=/Applications"

export DOCKER_TLS_VERIFY=1
export DOCKER_HOST=tcp://192.168.59.103:2376
export DOCKER_CERT_PATH=/Users/andre487/.boot2docker/certs/boot2docker-vm

# Imports
nvm_init="/usr/local/opt/nvm/nvm.sh"
gvm_init="~/.gvm/bin/gvm-init.sh"
fzf_init="~/.fzf.zsh"

[[ -f "$nvm_init" ]] && source "$nvm_init"
[[ -f "$gvm_init" ]] && source "$gvm_init"
[[ -f "$fzf_init" ]] && source "$fzf_init"

export PATH=$PATH:~/Library/Android/sdk/platform-tools
